{ 
    "env" : {
    "browser" : true,
    "node" : true,
    "es6" : true
    }
    ,
    "extends": ["eslint:recommended", "prettier"],
    "ecmaFeatures": {
        "modules": true,
        "spread" : true,
        "restParams" : true
    },

    "rules" : {
        "no-unused-vars" : 2,
        "no-undef" : 2
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8
    }
}